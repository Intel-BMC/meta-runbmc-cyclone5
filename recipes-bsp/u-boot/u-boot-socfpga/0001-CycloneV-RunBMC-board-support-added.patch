diff --git a/arch/arm/dts/socfpga_cyclone5_runbmc.dts b/arch/arm/dts/socfpga_cyclone5_runbmc.dts
new file mode 100644
index 0000000000..717e52a064
--- /dev/null
+++ b/arch/arm/dts/socfpga_cyclone5_runbmc.dts
@@ -0,0 +1,82 @@
+// SPDX-License-Identifier: GPL-2.0
+// Copyright (c) 2021 Intel Corporation
+
+#include "socfpga_cyclone5.dtsi"
+
+/ {
+	model = "Intel Cyclone V RunBMC";
+	compatible = "intel,cyclone5-runbmc", "altr,socfpga-cyclone5", "altr,socfpga";
+
+	chosen {
+		bootargs = "earlyprintk";
+		stdout-path = "serial0:115200n8";
+	};
+
+	memory {
+		name = "memory";
+		device_type = "memory";
+		reg = <0x0 0x40000000>; /* 1GB */
+	};
+
+	aliases {
+		/* this allow the ethaddr uboot environmnet variable contents
+		 * to be added to the gmac1 device tree blob.
+		 */
+		ethernet0 = &gmac1;
+		ethernet1 = &gmac0;
+	};
+
+	regulator_3_3v: 3-3-v-regulator {
+		compatible = "regulator-fixed";
+		regulator-name = "3.3V";
+		regulator-min-microvolt = <3300000>;
+		regulator-max-microvolt = <3300000>;
+	};
+};
+
+&gmac0 {
+	status = "okay";
+	/* Using MII-to-RMII SoftIP converter, hence from EMAC
+	perspective MII is used. PHY works in unmanaged mode,
+	set fixed speed to 100Mbps */
+	phy-mode = "mii";
+	fixed-link {
+		speed = <100>;
+		full-duplex;
+	};
+};
+
+&gmac1 {
+	status = "okay";
+	phy-mode = "rgmii-id";
+
+	txd0-skew-ps = <0>;
+	txd1-skew-ps = <0>;
+	txd2-skew-ps = <0>;
+	txd3-skew-ps = <0>;
+	txen-skew-ps = <0>;
+	txc-skew-ps = <1860>;
+	rxc-skew-ps = <1860>;
+};
+
+&gpio0 {
+	status = "okay";
+};
+
+&gpio1 {
+	status = "okay";
+};
+
+&gpio2 {
+	status = "okay";
+};
+
+&mmc0 {
+	/delete-property/ cap-sd-highspeed;
+	/delete-property/ broken-cd;
+	non-removable;
+	bus-width = <8>;
+	vmmc-supply = <&regulator_3_3v>;
+	vqmmc-supply = <&regulator_3_3v>;
+	status = "okay";
+};
