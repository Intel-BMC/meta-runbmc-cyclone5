+			"setexpr cnt ${cnt} + 1; " \
+			"gpt setenv mmc 0 rofs-${bootside}; " \
+			"mmc write ${loadaddr} ${gpt_partition_addr} ${cnt}; " \
+		"fi\0" \
+	"mmc_factory_init=" \
+		"run do_partitions && " \
+		"run mmc_update_uboot && " \
+		"run mmc_update_kernel && " \
+		"run mmc_update_rofs && " \
+		"setenv rwreset true && " \
+		"saveenv\0" \
+	"setbootargs=setenv bootargs " \
+		"console=ttyS0,115200n8 earlycon " \
+		"rootwait root=PARTLABEL=${rootfs}\0" \
+	"bootside=a\0" \
+	"bootcmd_mmc_a=" \
+		"echo Booting side A...; " \
+		"load mmc 0:2 ${scriptaddr} fitImage; " \
+		"setenv rootfs rofs-a; run setbootargs; " \
+		"bootm ${scriptaddr}; " \
+		"setenv rootfs\0" \
+	"bootcmd_mmc_b=" \
+		"echo Booting side B...; " \
+		"load mmc 0:3 ${scriptaddr} fitImage; " \
+		"setenv rootfs rofs-b; run setbootargs; " \
+		"bootm ${scriptaddr}; " \
+		"setenv rootfs\0" \
+	"bootcmd_mmc=" \
+		"if test ${bootside} = b; then " \
+			"run bootcmd_mmc_b; run bootcmd_mmc_a; " \
+		"else " \
+			"run bootcmd_mmc_a; run bootcmd_mmc_b; " \
+		"fi\0"
+
+/* The rest of the configuration is shared */
+#include <configs/socfpga_common.h>
+
+#endif	/* __CONFIG_SOCFPGA_CYCLONE5_RUNBMC_H__ */
